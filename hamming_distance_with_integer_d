__author__ = 'Fawad'
# This code calculate the hamming distance for a pattern with less than or equal to an integer d

# Function to calculate the hamming distance between the two sequences
def calc_hamming_distance(sequence1, sequence2):
    hamming_number = 0

    for nucleotide1, nucleotide2 in zip(sequence1, sequence2):
        if nucleotide1 != nucleotide2:
            hamming_number +=1

    return (hamming_number)

Pattern = "CACCTCAGGA"
Sequence= ""

# Max allowed difference in the number of the nucleotides in given pattern and the chunk of sequence of pattern's length
d = 5

matching_list = []

for i in range(len(Sequence) - len(Pattern) + 1):
    substring = Sequence[i:i + len(Pattern)]

    hamming_distance = calc_hamming_distance(Pattern, substring)
    if calc_hamming_distance(Pattern, substring) <= d:
        matching_list.append(i)

print(" ".join(map(str, matching_list)))
